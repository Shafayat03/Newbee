/// bubble sort sorting algo __> complexity -- > O(n * n)

#include<bits/stdc++.h>
using namespace std;

void bubbleSort(int arr[], int n){

    for(int pass = 0; pass < n -1; pass++){
        for(int i = 0; i+1 < n; i++){
            if(arr[i] > arr[i+1]){
                swap(arr[i],  arr[i+1]);
            }
        }
    }
}

 int main(){
    
  int arr[5] = {5,3,4,2,1};

  bubbleSort(arr, 5);

  for(int i = 0; i < 5; i++){
    cout << arr[i] << ' '; 
  }
  cout << "\n";

 }
